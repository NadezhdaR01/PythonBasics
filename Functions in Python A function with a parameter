def get_matrix(n, m, value):
    """
    функция создает матрицу и возвращает её
    n - количество строк
    m - количество столбцов
    value - значение для заполнения матрицы
    """
    matrix = []
    for i in range(n):
        matrix.append([])
        for j in range(m):
            matrix[i].append(value)
    return matrix


# Вызов функции
result1 = get_matrix(2, 2, 10)
result2 = get_matrix(3, 5, 42)
result3 = get_matrix(4, 2, 13)

# вывод результата работы функции в консоль
print(result1)
print(result2)
print(result3)
