# module_1_6.py
my_dict = {"Hope": 2001, "Kamila": 2003, "Egor": 2002}  # создание словаря(пары ключ-значение) 
print(my_dict)  # вывод словаря на экран
print(my_dict.get("Kamila"))  # Вывод значения на экран по существующему ключу 
print(my_dict.get("Nika", "Такого ключа нет"))  # Вывод значения на экран по отсутствующему ключу без ошибки
my_dict.update({"Axel" : 2005, "Nina" : 1999})  # Добавление в словарь двух новых пар через метод update()
delete_item = my_dict.pop("Nina")  # Удаление пары в словаре по существующему ключу 
print(delete_item)  # Выведения значения удаленной пары
print(my_dict)  # Вывод измененного словаря 

my_set = {True, 876, 4.7, "Remove", True, 876, 4.7, "Remove", 4}  # Создания множества с разными типами данных и повторяющимися значениями
print(my_set)  # отображаются только уникальные значения
# добавляем два элемента в множество
my_set.add(678) 
my_set.add("Dog")
# Удаляем элемент из множества, можно и с помощью метода remove
# но метод discard не выдаст ошибки, если элемента нет в множестве
my_set.discard(876)
print(my_set)  # Вывод измененного множества
